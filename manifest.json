{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "img/offline.png"
   },
   "description": "Proxy Anywhere allows you to instantaneously change to your custom proxy from a click of a button.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/online.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18",
   "name": "Proxy Anywhere",
   "options_page": "options.html",
   "permissions": [ "proxy" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.1.0"
}
